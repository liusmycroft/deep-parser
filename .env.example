# Example environment variables for RAG Testing System
# Copy this file to .env and adjust values as needed

# Database Configuration
RAG_DATABASE__ELASTICSEARCH__HOSTS='["http://localhost:9200"]'
RAG_DATABASE__ELASTICSEARCH__INDEX_NAME=rag_chunks
# RAG_DATABASE__ELASTICSEARCH__USERNAME=elastic
# RAG_DATABASE__ELASTICSEARCH__PASSWORD=changeme

RAG_DATABASE__MILVUS__HOST=localhost
RAG_DATABASE__MILVUS__PORT=19530
RAG_DATABASE__MILVUS__COLLECTION_NAME=rag_chunks

RAG_DATABASE__CLICKHOUSE__HOST=localhost
RAG_DATABASE__CLICKHOUSE__PORT=9000
RAG_DATABASE__CLICKHOUSE__DATABASE=rag_system

RAG_DATABASE__HBASE__HOST=localhost
RAG_DATABASE__HBASE__PORT=9090

# Embedding Configuration
RAG_EMBEDDING__MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2
RAG_EMBEDDING__MODEL_TYPE=sentence-transformers
RAG_EMBEDDING__BATCH_SIZE=32
# RAG_EMBEDDING__API_KEY=sk-...

# Processing Configuration
RAG_PROCESSING__CHUNK_SIZE=512
RAG_PROCESSING__CHUNK_OVERLAP=50
RAG_PROCESSING__MAX_DOCUMENT_SIZE=5000
# RAG_PROCESSING__IMAGE_TO_TEXT_API_URL=http://localhost:8080/convert
# RAG_PROCESSING__IMAGE_TO_TEXT_API_KEY=your-api-key

# Logging Configuration
RAG_LOGGING__LEVEL=INFO
RAG_LOGGING__FORMAT=json
RAG_LOGGING__ENABLE_CONSOLE=true
# RAG_LOGGING__OUTPUT_FILE=logs/rag_system.log

# Web API Configuration
RAG_WEB__HOST=0.0.0.0
RAG_WEB__PORT=8000
RAG_WEB__ENABLE_CORS=true
RAG_WEB__ENABLE_AUTH=false
# RAG_WEB__JWT_SECRET=your-secret-key
